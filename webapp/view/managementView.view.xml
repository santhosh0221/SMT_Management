									<!--	</moreBlocks>-->
									<!--</ObjectPageSubSection>-->
								</subSections>
							</ObjectPageSection>
						</sections>
					</ObjectPageLayout>
				</m:content>
			</m:Page>
		</m:pages>
	</m:App>
</mvc:View>