	<m:App>
		<m:pages>
			<m:Page showHeader="false">
				<m:content>
					<ObjectPageLayout id="ObjectPageLayout" showTitleInHeaderContent="true">
						<headerTitle>
							<ObjectPageDynamicHeaderTitle>
								<expandedHeading>
									<m:Title text="Mangement" wrapping="true"/>
								</expandedHeading>
								<snappedHeading>
									<m:FlexBox fitContainer="true" alignItems="Center">
										<m:Avatar src="https://www.signiwis.com/images/margo.png" backgroundColor="Random" class="sapUiTinyMarginEnd"/>
										<m:Title text="Denise Smith" wrapping="true"/>
									</m:FlexBox>
								</snappedHeading>
								<expandedContent>
									<m:Text text="Human resources"/>
								</expandedContent>
								<snappedContent>
									<m:Text text="Human resources"/>
								</snappedContent>
								<snappedTitleOnMobile>
									<m:Title text="Header Title On Phone"/>
								</snappedTitleOnMobile>
								<actions>
									<!--<m:Button icon="sap-icon://nav-back" press="onPressLogout"/>-->
									<m:Button icon="sap-icon://add" text="Emp" press="onPressAddEmp"/>
									<m:Button icon="sap-icon://add" text="EVent" press="onPressAddEvent"/>
									<m:Button icon="sap-icon://message-popup" press="onPressNotification"/>
								</actions>
							</ObjectPageDynamicHeaderTitle>
						</headerTitle>
						<headerContent>
							<m:FlexBox wrap="Wrap">
								<m:Avatar src="sap-icon://picture" backgroundColor="Random" displaySize="L" class="sapUiTinyMarginEnd"/>
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
									<m:ObjectStatus title="User ID" text="Emp0087"/>
									<m:ObjectStatus title="Language" text="English"/>
									<m:ObjectStatus title="Country" text="INDIA"/>
									<m:ObjectStatus title="Phone Number" text="1-987-654-3210"/>
								</layout:VerticalLayout>
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
									<m:ObjectStatus title="Functional Area" text="Mangement"/>
									<m:ObjectStatus title="Cost Center" text="Signiwis Mangement and Product"/>
									<m:ObjectStatus title="Email" text="Mangement@signiwis.com"/>
								</layout:VerticalLayout>
							</m:FlexBox>
						</headerContent>
						<sections>
							<ObjectPageSection title="Personal">
								<subSections>
									<ObjectPageSubSection title="Contact">
										<blocks>
											<m:HBox>
												<m:VBox>
													<f:SimpleForm layout="ColumnLayout" width="100%">
														<!--<m:Label text="Address"/>-->
														<core:Title text="Phone Numbers"/>
