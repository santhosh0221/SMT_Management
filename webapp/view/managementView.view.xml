														<m:Text text="+ 91-804-953-6364"/>
														<m:Label text="Office phone"/>
														<m:Text text="+ 91-804-953-6364"/>
													</f:SimpleForm>
												</m:VBox>
												<m:VBox>
													<f:SimpleForm layout="ColumnLayout" width="50%">
														<!--<m:Label text="Address"/>-->
														<core:Title text="Address"/>
														<m:Label text="Email: info@signiwis.com"/>
														<m:Text text="info@signiwis.com"/>
														<m:Label text="Get In Touch"/>
														<m:Text text="Signiwis Technologies Pvt. Ltd.#579, 3rd Floor, MG Complex,Above PMC Bank, 2nd Main,2nd Block, Rajajinagar,Bengaluru - 560010"/>
													</f:SimpleForm>
												</m:VBox>
											</m:HBox>
											<!--<personal:BlockSocial id="social"/>-->
											<!--<personal:BlockAdresses id="adresses"/>-->
