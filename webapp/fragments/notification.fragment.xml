<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:u="sap.ui.unified">
	<ResponsivePopover title="{i18n>Notifications}" id="fileUploader1" contentWidth="200px" placement="Bottom">
		<content>
			<List items="{DOB>/notificationDatas}" class="notification">
				<NotificationListItem title="{DOB>EmpId}" description="{DOB>requestFile}" authorName="{DOB>empName}" press="onNotificationTimeSheetPress"
					showCloseButton="false"/>
			</List>
		</content>
	</ResponsivePopover>
</core:FragmentDefinition>